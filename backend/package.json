{
  "name": "clob-trading-platform-backend",
  "version": "1.0.0",
  "description": "High-performance Redis-optimized backend for CLOB Trading Platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docker:build": "docker build -t clob-backend .",
    "docker:run": "docker run -p 3001:3001 -p 3002:3002 clob-backend",
    "redis:start": "redis-server",
    "redis:monitor": "redis-cli monitor",
    "benchmark": "node benchmarks/performance.js"
  },
  "keywords": [
    "aptos",
    "blockchain",
    "clob",
    "trading",
    "orderbook",
    "redis",
    "defi",
    "cryptocurrency"
  ],
  "author": "CLOB Trading Platform Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "redis": "^4.6.7",
    "ws": "^8.13.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.7.1",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "joi": "^17.9.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/clob-trading-platform.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/clob-trading-platform/issues"
  },
