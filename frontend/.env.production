# Production Environment Variables for CLOB Trading Platform
# These variables will be configured in Vercel dashboard

# API Configuration
REACT_APP_API_URL=https://api-clob-trading.vercel.app
REACT_APP_WS_URL=wss://api-clob-trading.vercel.app

# Aptos Network Configuration
REACT_APP_APTOS_NETWORK=testnet
REACT_APP_APTOS_NODE_URL=https://fullnode.testnet.aptoslabs.com/v1
REACT_APP_APTOS_FAUCET_URL=https://faucet.testnet.aptoslabs.com

# Contract Addresses (will be updated after deployment)
REACT_APP_CONTRACT_ADDRESS=0d5c0d1dc99dd0f4d7ec1dcd074532df8f53dc13c48ac28cdb7c4e3205612944

# Application Configuration
REACT_APP_ENVIRONMENT=production
REACT_APP_APP_NAME=CLOB Trading Platform
REACT_APP_VERSION=1.0.0

# Trading Configuration
REACT_APP_DEFAULT_TRADING_PAIR=APT-USDC
REACT_APP_SUPPORTED_TOKENS=APT,USDC,USDT,BTC,ETH

# Analytics and Monitoring
REACT_APP_ANALYTICS_ENABLED=true
REACT_APP_ERROR_REPORTING_ENABLED=true

# Feature Flags
REACT_APP_CROSS_CHAIN_ENABLED=true
REACT_APP_ADVANCED_CHARTS_ENABLED=true
REACT_APP_LIQUIDATION_PROTECTION_ENABLED=true
REACT_APP_PARALLEL_EXECUTION_ENABLED=true

# Performance Configuration
REACT_APP_CACHE_ENABLED=true
REACT_APP_WEBSOCKET_RECONNECT_INTERVAL=5000
REACT_APP_API_TIMEOUT=10000

# Security Configuration
REACT_APP_CSP_ENABLED=true
REACT_APP_HTTPS_ONLY=true

# Build Configuration
GENERATE_SOURCEMAP=false
CI=false
SKIP_PREFLIGHT_CHECK=true